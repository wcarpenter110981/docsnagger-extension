{
  "manifest_version": 3,
  "name": "DocSnagger",
  "version": "0.1",
  "description": "Internal Chrome extension to manage and sync client bank and vendor metadata.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://api.github.com/*"
  ]
}
